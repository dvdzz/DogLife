<% @dogs.each do |dog| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= link_to dog.name, dog_path(dog) %></h1><br />
    <i><%= dog.caption %></i><br /><br />

    <% if dog.user.present? %>
      <small><%= dog.user.email %></small>
    <% end %>

  </div>
<% end %>




<%= will_paginate @page, previous_label: h("<"), next_label: h(">") %>