<div class="booyah-box col-10 offset-1">
  <h1><%= @dog.name %></h1>
  <br/>
  <p><%= @dog.caption %></p>
  <div class="float-left">
    <% if current_user && current_user == @dog.user %>
        <%= link_to 'Edit', edit_dog_path(@dog), class: 'btn btn-primary' %>
    <% end %>
   <%= link_to 'Destroy', dog_path(@dog), method: :delete, data: {confirm: 'Are you sure you want to delete this?'},class: 'btn btn-danger' %>
  </div>
</div>